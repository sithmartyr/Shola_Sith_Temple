textures/Shola_Temple/invisilight_white
{
   qer_editorimage	textures/system/invisilight_image.jpg
   qer_trans	0.7
   surfaceparm nomarks
   surfaceparm nonsolid
   surfaceparm nonopaque
   surfaceparm trans
   surfaceparm nolightmap
   q3map_lightRGB 1 1 1
   q3map_nolightmap
   q3map_surfacelight 350
   {
      map $whiteimage
      rgbGen const ( 0.000000 0.000000 0.000000 )
      blendFunc GL_ONE GL_ONE
   }
}

